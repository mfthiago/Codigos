#include <stdio.h>

int main(void) {

  int mat[5][5], i, j, soma=0;

  for (i=0;i<5;i++){
    for (j=0;j<5;j++){
      printf("Informe o valor da matriz: ");
      scanf("%d", &mat[i][j]);
    }
  }
  for (i=0;i<5;i++){
    soma += mat[i][2];
  }
  printf("A soma dos elementos da 2° coluna é: %d", soma);
  
  
  
  return 0;
}
