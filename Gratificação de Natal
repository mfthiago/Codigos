he = int(input('Digite o número de horas extras trabalhadas: '))
hf = int(input('Digite o número de horas faltas: '))
he = he * 60
hf = hf * 60
h = he - (2/3 * (hf))
if h >=2401:
    print("O prêmio é de R$500")
elif h >=1801:
    print("O prêmio é de R$400")
elif h >=1201:
    print("O prêmio é de R$300")
elif h >= 600:
    print("O prêmio é de R$200")
elif h<600:
    print("O prêmio é de R$100")
