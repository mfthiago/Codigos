#include <stdio.h>

int main(void) {

  int vet[10], i;

  for(i=0;i<10;i++){
    printf("Digite o valor da %d° posição: ", i+1);
    scanf("%d", &vet[i]);
  }
  printf("Elementos ímpares do vetor: ");
  for(i=0; i<10; i++){
    if (vet[i] % 2 !=0){
      printf("%d\n",vet[i]);
    }
  }
  return 0;
}
